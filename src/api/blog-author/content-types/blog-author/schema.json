{
  "kind": "collectionType",
  "collectionName": "blog_authors",
  "info": {
    "singularName": "blog-author",
    "pluralName": "blog-authors",
    "displayName": "Blog Author"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "blog_author": {
      "type": "string",
      "default": "Author Name",
      "required": true,
      "unique": true
    },
    "blogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog",
      "mappedBy": "blog_author"
    }
  }
}
